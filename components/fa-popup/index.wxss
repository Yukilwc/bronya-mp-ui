.popupContainer .mask {
  position: fixed;
  z-index: 1000;
  top: 0px;
  left: 0px;
  right: 0px;
  bottom: 0px;
  width: 100%;
  height: 100%;
  background: black;
  opacity: 0;
}

.popupContainer .wrapper {
  position: fixed;
  z-index: 1001;
  bottom: 0px;
  width: 100%;
  transform: translate3d(0, 100%, 0);
}
